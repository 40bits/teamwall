  "instruments": [
    {
      "id": "title",
      "title": "Title area",
      "instrument": "textarea",
      "url": "data/textArea.json",
      "fontSize": 40,
      "color": "#eeeeee",
      "text_align" : "center"
    },
    {
      "id": "codecoverage",
      "instrument": "percent",
      "title": "Code Coverage",
      "url": "data/codecoverage.json",
      "threshold_value": "80",
      "higher_is_better": true,
      "show_trend": true
    },
    {
      "id": "greenbuildpercentage",
      "instrument": "percent",
      "title": "Green Build",
      "url": "data/greenbuildpercentage.json",
      "threshold_value": "90",
      "higher_is_better": true,
      "show_trend": false
    },
    {
      "id": "buildchain",
      "title": "Webshop Buildchain",
      "instrument": "buildchain",
      "url": "data/buildchain.json"
    },
    {
      "id": "violations",
      "instrument": "number",
      "title": "Code Violations",
      "url": "data/codeviolations.json",
      "threshold_value": "5",
      "decimal_places": "0",
      "higher_is_better": false,
      "unit": "#",
      "show_trend": true
    },
    {
      "id": "xs",
      "instrument": "percent",
      "title": "Structural Complexity",
      "url": "data/xs.json",
      "threshold_value": 4,
      "higher_is_better": false,
      "show_trend": true
    },
    {
      "title": "Broken Builds",
      "showCount": true,
      "instrument": "buildalert",
      "id": "buildalert",
      "sounds": {
        "failureSound": "/sounds/tos-redalert.mp3",
        "failureInterval": "1800",
        "successSound": "/sounds/smb3_power-up.wav"
      },
      "url": "data/buildalert.json"
    },
    {
      "id": "issuecount",
      "instrument": "issuecount",
      "title": "Issues",
      "priorityColors": [ "#ff0000", "#FF5A36", "#FF7F00", "#FFA812" ],
      "url": "data/issuecount.json"
    }
  ]
